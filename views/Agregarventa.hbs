<body>
    {{> bar}}
    <br><br><br>
    <center>
        <h2>Agregar una nueva venta</h2>
        <div class="card" style="width: 40%;">
            <div class="card-header" style="text-align: left;">
                <a class="btn-close end" href="ventas"></a>
            </div>
            <div class="card-body">
                <form>
                    <div class="row">
                        <div class="col-6">
                            <label>Id</label>
                            <input type="number">
                        </div>
                        <div class="col-6">
                            <label>Nombre</label>
                            <input type="text">
                        </div>
                    </div>
                    <br>
                    <div class="row">
                        <div class="col-6">
                            <label>Teléfono</label>
                            <input type="number">
                        </div>
                        <div class="col-6">
                            <label>Producto</label>
                            <input type="text">
                        </div>
                    </div>
                    <br>
                    <div class="row">
                        <div class="col-6">
                            <label>Total</label>
                            <input type="number">
                        </div>
                        <div class="col-6">
                            <label>Fecha</label><br>
                            <input type="date">
                        </div>
                    </div>

            </div>
            <br>
            <a class="btn btn-danger" href="ventas">Cancelar</a>
            <a class="btn btn-success">Enviar</a>
            </form>
        </div>
        </div>
    </center>
    <br>

</body>
