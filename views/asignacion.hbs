{{> navbarDashboard}}
<br>
<center>
    <div class="card" style="width: 40%;">
        <div class="card-body">
            <form>
                <div class="col-4">
                    <label>Id Asignacion</label>
                    <input type="number" id="idAsignacion">
                </div>
                <br>
                <div class="col-4">
                    <label>Id Rol</label>
                    <input type="number" id="idRol">
                </div>
                <br>
                <div class="col-4">
                    <label>Id Permiso</label>
                    <input type="number" id="idPermiso">
                </div>
                <br>
                <div class="col-4">
                    <label>Estado</label>
                    <input type="text" id="estado">
                </div>
                <br>
                <button type="button" id="boton" class="btn btn-primary">Enviar</button>
            </form>
        </div>
    </div>
</center>
<script src="assets/js/asignacionValidar.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js"
