    <center>
        <div class="container" style="margin-top: 10px;">
            <div class="card" style="width: 95%; border-radius: 0;">
                <div class="card-body">
                    <h4 class="card-title" style="margin-bottom: 2%;">Registrar Compra</h4>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="card" style="width: 80%; border-radius: 0;">
                                <div class="card-body">
                                    <form>
                                        <div class="form-floating mb-3">
                                            <input type="text" class="form-control" list="P_Registrados"
                                                id="floatingInput" style="border-radius: 0;" placeholder="Empresa X">
                                            <datalist id="P_Registrados">
                                                <option value="El Hueco">
                                                <option value="La Minorista">
                                                <option value="El Centro">
                                                <option value="...">
                                            </datalist>
                                            <label for="floatingInput">Proveedor</label>
                                        </div>
                                        <div class="form-floating mb-3">
                                            <input type="text" class="form-control" id="floatingPassword"
                                                placeholder="Id/# Recibo" style="border-radius: 0;">
                                            <label for="floatingPassword">Recibo</label>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-5">
                            <div class="card" style="width: 155%; border-radius: 0;">
                                <div class="card-body">
                                    <h5 class="card-title" style="margin-bottom: 2%;">Agregar Insumo a la compra</h5>
                                    <form>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-floating mb-3">
                                                    <input type="text" class="form-control" list="I_Registrados"
                                                        id="floatingInput" placeholder="Insumo XYZ" style="border-radius: 0;">
                                                    <datalist id="I_Registrados">
                                                        <option value="Pliego de cuero">
                                                        <option value="Hilo">
                                                        <option value="Botones">
                                                        <option value="Cierres">
                                                        <option value="Clavos">
                                                        <option value="Tijeras">
                                                        <option value="...">
                                                    </datalist>
                                                    <label for="floatingInput">Insumo</label>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-floating mb-3">
                                                    <input type="number" class="form-control" id="floatingPassword"
                                                        placeholder="Cantidad" style="border-radius: 0;">
                                                    <label for="floatingPassword">Cantidad</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-4">
                                                <div class="form-floating mb-3">
                                                    <input type="number" class="form-control" id="floatingPassword"
                                                        placeholder="Precio" style="border-radius: 0;">
                                                    <label for="floatingPassword">PrecioUND</label>
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="form-floating mb-3">
                                                    <input type="number" class="form-control" id="floatingPassword"
                                                        placeholder="P_Iva" style="border-radius: 0;">
                                                    <label for="floatingPassword">%IVA</label>
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="form-floating mb-3">
                                                    <input type="number" class="form-control" id="floatingPassword"
                                                        placeholder="Iva" style="border-radius: 0;">
                                                    <label for="floatingPassword">IVA</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="d-grid gap-2">
                                            <button class="btn btn-dark" style="border-radius: 0;" type="button">Agregar</button>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div style="margin-top: 4%;">
                        <div class="card" style="width: 100%; border-radius: 0;">
                            <div class="card-body">
                                <h5 class="card-title" style="margin-bottom: 2%;">Insumos de la compra</h5>
                                <table class="table">
                                    <thead class="table-dark">
                                        <tr style="text-align: center;">
                                            <th> Insumo</th>
                                            <th> Precio</th>
                                            <th> Porcentaje Iva</th>
                                            <th> Cantidad</th>
                                            <th> Monto </th>
                                            <th> Iva</th>
                                            <th> Subtotal </th>
                                            <th> Funciones </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr style="text-align: center;">
                                            <td>...</td>
                                            <td>...</td>
                                            <td>...</td>
                                            <td>...</td>
                                            <td>...</td>
                                            <td>...</td>
                                            <td>...</td>
                                            <td><i class="fa-sharp fa-solid fa-pen"></i>   <i class="fa-sharp fa-solid fa-trash" style="margin-left: 5px;"></i></td>
                                        </tr>
                                        <tr style="text-align: center;">
                                            <td>...</td>
                                            <td>...</td>
                                            <td>...</td>
                                            <td>...</td>
                                            <td>...</td>
                                            <td>...</td>
                                            <td>...</td>
                                            <td><i class="fa-sharp fa-solid fa-pen"></i>   <i class="fa-sharp fa-solid fa-trash" style="margin-left: 5px;"></i></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div style="margin-top: 3%;">
                        <div class="row">
                            <div class="col-md-5">
                                <div class="card" style="width: 60%; border-radius: 0;">
                                    <div class="card-body">
                                        <form>
                                            <div class="form-floating mb-3">
                                                <input type="text" class="form-control" id="floatingInput" placeholder="Total" value="$$$$" style="border-radius: 0;" disabled>
                                                <label for="floatingInput">Total Compra</label>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6" style="margin-top: 1%;">
                                <div class="d-grid gap-2">
                                    <button class="btn btn-success" style="border-radius: 0;" type="button">Guardar Compra</button>
                                    <a class="btn btn-danger" href="compras" style="border-radius: 0;" type="button">Cancelar</a>
                                  </div>
                            </div>
                        </div>
                        <br>
                    </div>
                </div>
            </div>
        </div>
    </center>
<br><br>
</body>
