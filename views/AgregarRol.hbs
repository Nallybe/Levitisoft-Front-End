<body>
    {{> bar}}
    <br><br><br><br>
    <center>
        <h2>Agregar un nuevo rol</h2>
        <div class="card" style="width: 40%;">
            <div class="card-header" style="text-align: left;">
                <a class="fa fa-xmark" href="roles"></a>
            </div>
            <div class="card-body">
                <form>
                    <div class="col-6">
                        <label>Id Rol</label>
                        <input type="number" id="idRol">
                    </div>
                    <br>
                    <div class="col-6">
                        <label>Nombre Rol</label>
                        <input type="text" id="nombreRol">
                    </div>
                    <br>
                    <div class="col-6">
                        <label>Estado</label><br>
                        <select id="estadoRol">
                            <option value="Activo">Activo</option>
                            <option value="Inactivo">Inactivo</option>
                        </select>
                    </div>
                    <br>
                    <a class="btn btn-danger" href="roles">Cancelar</a>
                    <button type="button" id="boton" class="btn btn-success">Enviar</button>
                </form>
            </div>
        </div>
    </center>



