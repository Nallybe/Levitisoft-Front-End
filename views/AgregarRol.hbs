</head>

<body>
  <!--Barra de navegación-->
  <div class="wrapper ">
    <div class="sidebar" data-color="black">
      <!--
        Tip 1: You can change the color of the sidebar using: data-color="blue | green | orange | red | yellow"
    -->
      <div class="logo">
        <a href="" class="simple-text" style="color:white;">
          Levitisoft
        </a>
      </div>
      <div class="sidebar-wrapper" id="sidebar-wrapper">
        <ul class="nav">
          <li>
            <a href="dashboard">
              <i class="fa fa-house" style="color:white;"></i>
              <p style="color:white;">Home</p>
            </a>
          </li>
          <li class="active">
            <a href="roles" style="background-color: white">
              <i class="fa fa-gear" style="color: black;"></i>
              <p style="color: black;">Roles</p>
            </a>
          </li>
          <li>
            <a href="usuarios">
              <i class="fa fa-users" style="color:white;"></i>
              <p style="color:white;">Usuarios</p>
            </a>
          </li>
          <li>
            <a href="compras">
              <i class="fa fa-shop" style="color:white;"></i>
              <p style="color:white;">Compras</p>
            </a>
          </li>
          <li>
            <a href="insumos">
              <i class="fa fa-box" style="color:white;"></i>
              <p style="color:white;">Insumos</p>
            </a>
          </li>
          <li>
            <a href="producción">
              <i class="fa fa-scissors" style="color:white;"></i>
              <p style="color:white;">Producción</p>
            </a>
          </li>
          <li>
            <a href="productos">
              <i class="fa fa-boxes-packing" style="color:white;"></i>
              <p style="color:white;">Productos</p>
            </a>
          </li>
          <li>
            <a href="clientes">
              <i class="fa fa-user" style="color:white;"></i>
              <p style="color:white;">Clientes</p>
            </a>
          </li>
          <li>
            <a href="Carrito de compras">
              <i class="fa fa-cart-shopping" style="color:white;"></i>
              <p style="color:white;">Carrito de compras</p>
            </a>
          </li>
          <li>
            <a href="ventas">
              <i class="fa fa-truck-fast" style="color:white;"></i>
              <p style="color:white;">Ventas</p>
            </a>
          </li>
          <li>
            <a href="reparaciones">
              <i class="fa fa-screwdriver-wrench" style="color:white;"></i>
              <p style="color:white;">Reparaciones</p>
            </a>
          </li>
        </ul>
      </div>
    </div>
    <div class="main-panel" id="main-panel">
      <!-- Navbar -->
      <nav class="navbar navbar-expand navbar-absolute" style="background-color: rgb(124, 123, 123);">
        <div class="container-fluid">
          <div class="navbar-wrapper">
            <div class="navbar-toggle">
              <button type="button" class="navbar-toggler">
                <span class="navbar-toggler-bar bar1"></span>
                <span class="navbar-toggler-bar bar2"></span>
                <span class="navbar-toggler-bar bar3"></span>
              </button>
            </div>
            <a class="navbar-brand" href="#">Roles</a>
          </div>
          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navigation"
            aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-bar navbar-kebab"></span>
            <span class="navbar-toggler-bar navbar-kebab"></span>
            <span class="navbar-toggler-bar navbar-kebab"></span>
          </button>
          <div class="collapse navbar-collapse justify-content-end" id="navigation">
            <a class="nav-link" href="home">
              <i class="fa fa-user"></i>
              <p>
                <span class="d-lg-none d-md-block">Account</span>
              </p>
            </a>
          </div>
        </div>
      </nav>
      <!--Barra de navegación-->
      <br><br><br><br>
      <center>
        <h2>Agregar un nuevo rol</h2>
        <div class="card" style="width: 40%;">
          <div class="card-body">
            <br>
            <form>
              <div class="row">
                <div class="col-6" style="margin-left: 10px;">
                  <label>Nombre Rol</label>
                  <input type="text" id="nombreRol" required>
                </div>
                <div class="col-5">
                  <!--Permisos-->
                  <!-- Button trigger modal -->
                  <button type="button" class="btn btn-info" data-bs-toggle="modal" data-bs-target="#exampleModal1">
                    Permisos
                  </button>

                  <!-- Modal -->
                  <div class="modal fade" id="exampleModal1" tabindex="-1" aria-labelledby="exampleModalLabel"
                    aria-hidden="true">
                    <div class="modal-dialog">
                      <div class="modal-content">
                        <div class="modal-header">
                          <h4 class="modal-title fs-5" id="exampleModalLabel">Permisos</h4>
                          <button type="button" class="fa fa-xmark" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                          <div class="row">
                            <div class="col-6">
                              Roles
                            </div>
                            <div class="col-6">
                              <input type="checkbox" name="checkbox" class="bootstrap-switch" data-on-label="ON"
                                data-off-label="OFF" />
                            </div>
                          </div>
                          <div class="row">
                            <div class="col-6">
                              Usuarios
                            </div>
                            <div class="col-6">
                              <label class="switch ">
                                <input type="checkbox">
                                <span class="slider round"></span>
                              </label>
                            </div>
                          </div>
                          <div class="row">
                            <div class="col-6">
                              Compras
                            </div>
                            <div class="col-6">
                              <label class="switch ">
                                <input type="checkbox">
                                <span class="slider round"></span>
                              </label>
                            </div>
                          </div>
                          <div class="row">
                            <div class="col-6">
                              Insumos
                            </div>
                            <div class="col-6">
                              <label class="switch">
                                <input type="checkbox">
                                <span class="slider round"></span>
                              </label>
                            </div>
                          </div>
                          <div class="row">
                            <div class="col-6">
                              Producción
                            </div>
                            <div class="col-6">
                              <label class="switch">
                                <input type="checkbox">
                                <span class="slider round"></span>
                              </label>
                            </div>
                          </div>
                          <div class="row">
                            <div class="col-6">
                              Productos
                            </div>
                            <div class="col-6">
                              <label class="switch">
                                <input type="checkbox">
                                <span class="slider round"></span>
                              </label>
                            </div>
                          </div>
                          <div class="row">
                            <div class="col-6">
                              Clientes
                            </div>
                            <div class="col-6">
                              <label class="switch">
                                <input type="checkbox">
                                <span class="slider round"></span>
                              </label>
                            </div>
                          </div>
                          <div class="row">
                            <div class="col-6">
                              Carrito de compras
                            </div>
                            <div class="col-6">
                              <label class="switch">
                                <input type="checkbox">
                                <span class="slider round"></span>
                              </label>
                            </div>
                          </div>
                          <div class="row">
                            <div class="col-6">
                              Ventas
                            </div>
                            <div class="col-6">
                              <label class="switch">
                                <input type="checkbox">
                                <span class="slider round"></span>
                              </label>
                            </div>
                          </div>
                          <div class="row">
                            <div class="col-6">
                              Reparaciones
                            </div>
                            <div class="col-6">
                              <label class="switch">
                                <input type="checkbox">
                                <span class="slider round"></span>
                              </label>
                            </div>
                          </div>

                        </div>
                        <div class="modal-footer">
                          <div class="row">
                            <div class="col-6">
                              <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cerrar</button>

                            </div>
                            <div class="col-6">
                              <button type="button" class="btn btn-success" data-bs-dismiss="modal">Guardar</button>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <!--Permisos-->
              </div>
              <br>
              <div class="row">
                <div class="col-12">
                  <label>Estado</label><br>
                  <select id="estadoRol" required>
                    <option value="Activo">Activo</option>
                    <option value="Inactivo">Inactivo</option>
                  </select>
                </div>


              </div>
              <br>

              <br>
              <a class="btn btn-danger" href="roles">Cancelar</a>
              <button type="button" id="boton" class="btn btn-success">Enviar</button>
            </form>
          </div>
        </div>
      </center>



</body>
<!--Inicio footer-->
{{> footer}}
